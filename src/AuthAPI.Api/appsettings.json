{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowedOrigins": [
      "https://localhost:3000"
    ]
  },
  "ConnectionStrings": {
    "AuthAPI": "Host=host;Port=5432;Database=AuthAPI;Username=postgres;Password=password"
  },
  "JwtSettings": {
    "Key": "An at least 256 bits signing key",
    "Issuer": "AuthAPI",
    "Audience": "AuthAPI",
    "ExpirationMinutes": 15
  }
}